var stockTicker {

    _allTickers: null,
    _overallCounter: 0,

    _divIdForTicker = function(ticker) {
	return 'result-' + ticker + '-' + this._overallCounter;
    },

    tickerQueryCallBack = function(data) {
	$('#' + _divIdForTicker(ticker)).html(data.div);
	$('head').append(data.script);
    },

    processTickers = function(tickers, tickerQueryUrl) {
	this._overallCounter += 1;
	this._allTickers = tickers;

	var innerHtml = "";

	var i;
	for (i = 0; i < tickers.length; i++ ) {
	    innerHtml += '<div id="' + this._divIdForTicker(ticker[i]['ticker']) +
		'"></div>';
	    $.getJSON(tickerQueryUrl, { ticker: ticker[i]['ticker'] },
		      this.tickerQueryCallBack);		      
	}
	$('#results').html(innerHtml);
    },
};	    
	    
